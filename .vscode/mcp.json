{
	"servers": {
		"Framelink Figma MCP": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"figma-developer-mcp",
				"--figma-api-key=%FIGMA_API_KEY%",
				"--stdio"
			],
			"type": "stdio"
		},
		"TalkToFigma": {
			"command": "bunx",
			"args": [
				"cursor-talk-to-figma-mcp@latest",
				"--figma-api-key=%FIGMA_API_KEY%",
				"--channel=%FIGMA_CHANNEL%"
			],
			"type": "stdio"
		},
		"Memory MCP": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"@modelcontextprotocol/server-memory@latest"
			],
			"type": "stdio"
		},
		"Brave Search MCP": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"@modelcontextprotocol/server-brave-search@latest",
				"--api-key=%BRAVE_API_KEY%"
			],
			"type": "stdio"
		},
		"Puppeteer MCP": {
			"command": "cmd",
			"args": [
				"/c",
				"npx",
				"-y",
				"@modelcontextprotocol/server-puppeteer@latest"
			],
			"type": "stdio"
		}
	},
	"inputs": []
}